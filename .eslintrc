{
	"env": {
    "browser": true
	},
	"rules": {
    "no-with": 1,
    "no-eval": 1,
    "keyword-spacing": 1,

    "curly": [1, "all"],
    "space-before-blocks": 1,
    "no-empty": 1,

    "wrap-iife": 1,

    "object-curly-spacing": [1, "never"],
    "space-in-parens": [1, "never"],

    "space-infix-ops": 1,
    "dot-notation": 1,

    "camelcase": 1,
    "no-multi-str": 1,

    "no-mixed-spaces-and-tabs": 1,
    "no-trailing-spaces": 1,
    "eol-last" : 1,

    "comma-spacing": [1, {"before": false, "after": true}],
    "indent": [1, 2],
    "quotes": [1, "single"]
	}
}